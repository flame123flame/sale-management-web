{
  "name": "sale-manament-web",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "dev": "ng serve --host 0.0.0.0 --configuration=dev",
    "qa": "ng serve --host 0.0.0.0 --configuration=qa",
    "sit": "ng serve --host 0.0.0.0 --configuration=sit",
    "uat": "ng serve --host 0.0.0.0 --configuration=uat",
    "prod": "ng serve --host 0.0.0.0 --configuration=production",
    "build": "ng build --output-path=dist",
    "build:dev": "node build-version.js && ng build --base-href=/ --configuration=dev --build-optimizer --aot --output-hashing=all",
    "build:qa": "node build-version.js && ng build --base-href=/ --configuration=qa --build-optimizer --aot --output-hashing=all",
    "build:sit": "node build-version.js && ng build --base-href=/calcufy-config/ --configuration=sit --build-optimizer --aot --output-hashing=all",
    "build:uat": "node build-version.js && ng build --base-href=/ --configuration=uat --build-optimizer --aot --output-hashing=all",
    "build:prod": "node build-version.js && ng build --base-href=/ --configuration=production --build-optimizer --aot --output-hashing=all",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.0.0",
    "@angular/common": "^19.0.0",
    "@angular/compiler": "^19.0.0",
    "@angular/core": "^19.0.0",
    "@angular/forms": "^19.0.0",
    "@angular/platform-browser": "^19.0.0",
    "@angular/platform-browser-dynamic": "^19.0.0",
    "@angular/platform-server": "^19.0.0",
    "@angular/router": "^19.0.0",
    "@angular/ssr": "^19.0.4",
    "@ngx-translate/core": "^16.0.4",
    "express": "^4.18.2",
    "moment": "^2.30.1",
    "ng-zorro-antd": "^19.3.1",
    "ngx-device-detector": "^10.0.2",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "url": "^0.11.4",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.0.4",
    "@angular/cli": "^19.0.4",
    "@angular/compiler-cli": "^19.0.0",
    "@types/express": "^4.17.17",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^18.18.0",
    "jasmine-core": "~5.4.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.6.2"
  }
}
